pipeline{
    agent any
    tools {
  maven 'm1'
}
stages {
  stage('clone') {
    steps {
      checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: 'ssh-key-for-git', url: 'https://github.com/hubchai/Amazon.git']])
    }
  }

  stage('clean') {
    steps {
      sh 'mvn clean'
    }
  }


  stage('build') {
    steps {
      sh 'mvn install'
    }
  }

  stage('test') {
    steps {
      sh 'mvn test'
    }

    post {
      always {
        junit 'Amazon-Core/target/surefire-reports/*.xml'
      }
    }
  }

}

}

